{
  "name": "list-maker-server",
  "version": "1.0.0",
  "description": "A simple Typescript MERN stack application that allows unique users to create simple notes, with authenication.",
  "main": "dist/server.js",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.4"
  },
  "scripts": {
    "dev-server": "nodemon src/server.ts",
    "start": "node dist/server.js",
    "client": "npm --prefix ../client run dev",
    "dev": "concurrently \"npm run dev-server\" \"npm run client\"",
    "build": "npm i && tsc"
  },
  "keywords": [
    "MERN, Tailwind, Typescript, React, Vite, Express, MongoDB, Mongoose, jsonwebtoken"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.13",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
